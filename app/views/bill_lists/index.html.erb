<p id="notice"><%= notice %></p>

<h1>Bill Lists</h1><hr />
<% @bill_lists.each do |bill_list| %>
  <div class="row">
    <div class="col s12 m12">
      <div class="card blue-grey darken-1">
        <div class="card-content white-text">
          <span class="card-title"><%= bill_list.title %></span>
          <p><%= bill_list.description %></p>
        </div>
        <div class="card-action">
          <td><%= link_to 'Show', bill_list %></td>
          <td><%= link_to 'Edit', edit_bill_list_path(bill_list) %></td>
          <td><%= link_to 'Destroy', bill_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </div>
      </div>
    </div>
  </div>
<% end %>
